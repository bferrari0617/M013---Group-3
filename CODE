# Code file
def encode(p1: str) -> str:
    code = ""
    for char in p1:
        if char == "a":
            code += "10101"
        if char == "b":
            code += "0010101"
        if char == "c":
            code += "0010110"

        if char == "A":
            code += "10000"
        if char == "B":
            code += "0000000"
        if char == "C":
            code += "0000001"

        if char == ".":
            code += "11110"

    return code
